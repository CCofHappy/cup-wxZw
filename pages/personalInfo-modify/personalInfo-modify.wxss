/* personalInfo-modify.wxss */
page{
  background: #efefef;
}

.user-head{
  background: #fff;
  padding: 60rpx 0 100rpx 0; 
}

.user-head-img{
  width: 200rpx;
  height: 200rpx;
  margin:auto;
  position: relative; 
}

.user-head-img .box-center{
  width: 200rpx;
  height: 200rpx;
  border-radius: 50%;
  overflow: hidden;
  border: 2rpx solid #efefef;
}

.user-code{
  position: absolute;
  bottom: -40rpx;
  width: 100%;
  text-align: center;
  color: #876A2B;
}

.items-list{
  border-top: 2rpx solid #efefef;
}

.icon-camera{
  width: 44rpx;
  height: 44rpx;
  position: absolute;
  right: 0;
  top: 6rpx;
  border-radius: 50%;
  background-image: url(data:image/png;base64,)
}