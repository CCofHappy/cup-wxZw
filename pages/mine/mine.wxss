page {
    background: #efefef;
}

.header {
    position: relative;
    color: #fff;
    overflow: hidden;
    background-size: 100% auto;
    background-position-y: center;
}

.header::before {
    content: '';
    position: absolute;
    width: 100%;
    height: 100%;
    background: #000;
    opacity: 0.5;
}

.header-box {
    position: relative;
    z-index: 10;
}

.header-img {
    padding: 40rpx 24rpx;
    border-top: 2rpx solid #efefef;
    border-bottom: 2rpx solid #efefef;
}

.header-img .header-nav {
    height: 120rpx;
    width: 120rpx;
    border-radius: 50%;
    border: 4rpx solid #fff;
    overflow: hidden;
}

.header-text {
    padding: 20rpx 10rpx;
    font-weight: bold;
}

.user-name {
    font-weight: normal;
    font-size: 34rpx;
    padding: 10rpx 0 0;
}

.header-footer {
    width: 100%;
    padding: 10rpx 0 20rpx;
    margin-bottom: 20rpx; 
}
.header-order-box {
    width: 100%;
    padding: 30rpx 0 20rpx;
    border-bottom: 2rpx solid #efefef;
}
.header-footer .count{
    font-size: 32rpx;
}
.header-footer .count-txt{
    color: #9B9B9B;
}
.header-footer .box-flex {
    padding: 20rpx 0;
    position: relative;
}

.header-footer .box-flex .badge {
    top: 10rpx;
    right: 36rpx;
}
.icon-header {
    width: 40rpx;
    height: 40rpx;
    background-size: 100%;
    background-repeat: no-repeat;
    background-position: center;
    margin: 6rpx 0;
}
.icon-other {
    width: 40rpx;
    height: 40rpx;
    background-size: 100%;
    background-repeat: no-repeat;
    background-position: center;
    margin-right: 20rpx;
}

.other-items .items-list {
    border-bottom: 2rpx solid #efefef;
    height: 100rpx;
    line-height: 100rpx;
}
.text-content{
    display: none;
}
.text-content .weui-flex__item {
    padding: 30rpx 0;
}
.text-content .txt{
    font-size: 24rpx;
    color: #656565;
}
.text-content.open{
  display: block;
  border-bottom: 2rpx solid #efefef;
}
.setting {
    position: absolute;
    right: 30rpx;
    top: 30rpx;
    z-index: 9999;
}

.user-prof {
    font-size: 24rpx;
    font-weight: 400;
    width: 400rpx;
    margin: auto;
}

