@import '../../component/callPrice/callPrice.wxss';
@import '../../component/oneHand/oneHand.wxss';

page {
    background: #efefef;
}

.container {
    padding-bottom: 100rpx;
}

.swiper {
    width: 100%;
}
.swiper-img-container{
    position: relative;
}
.swiper-img-container .auction-state{
    position: absolute;
    bottom: 0;
    left: 0;
    width: 100%;
    background: rgba(148, 44, 21, 0.8);
    color: #fff;
    height: 80rpx;
    line-height: 80rpx;
    align-items: center;
    font-size: 32rpx;
}
.auction-state-desc{
    padding: 0 20rpx;
}
.slide-image {
    width: 100%;
}
.icon-pro {
    width: 25rpx;
    height: 25rpx;
}

.pro-info-text {
    padding: 20rpx 20rpx 0;
    margin-bottom: 10rpx;
}
.pro-info-tips{
    background:#fff;
    color: #656565;
    padding:20rpx;
    margin:20rpx 0;
}
.pro-info-tips .font-bold{
    color: #942C15;
    font-size: 36rpx;
    margin-bottom: 20rpx;
}
.pro-name {
    font-size: 42rpx;
    font-weight: bold;
}
.pro-price-box {
    line-height: 88rpx;
}
.pro-labels .other-box:first-child {
    border-top: 2rpx solid #efefef;
    border-bottom: 2rpx dashed #efefef;
}
.other-box {
    line-height: 55rpx;
}
.pro-item-list{
    margin:20rpx 0;
}
.pro-item-list .items-list{
    border-bottom: 2rpx solid #efefef;
    color: #282828;
    font-weight: bold;
    font-size: 32rpx;
}
.text-content{
    display: none;
}
.text-content.open{
  display: block;
  border-bottom: 2rpx solid #efefef;
}
.footer-bar {
    height: 100rpx;
    width: 100%;
    background: #fff;
    position: fixed;
    bottom: 0;
}

.footer-bar .icon {
    width: 40rpx;
    height: 40rpx;
    margin: auto;
}

.footer-bar>.box:nth-child(1) {
    width: 33%;
}

.footer-bar>.box:nth-child(2) {
    line-height: 100rpx;
    width: 67%;
    color: #fff;
    background: #c72202;
}

.one-hand-btn {
    background: #A72125;
}
.right-btn{
    background: #942C15;
}

.pro-info-detail {
    padding-bottom: 20rpx;
}

.pro-content-box {
    padding-bottom: 20rpx;
}
.pro-content-box .title{
    font-size: 32rpx;
    color: #282828;
    font-weight: bold;
    padding: 20rpx;
}
.pro-item-label {
    color: #9a9a9a;
    border: 2rpx solid #9a9a9a;
    padding: 0 4rpx;
    border-radius: 3px;
    font-size: 20rpx;
    margin: 6rpx 6rpx 6rpx 0;
}
.pro-rule {
    padding: 20rpx;
}

.pro-rule>.box {
    line-height: 70rpx;
    border-bottom: 2rpx solid #efefef;
}

.info-table {
    color: #000;
    margin: 0 20rpx 20rpx;
    border: 2rpx solid #efefef;
}

.info-table .table-list {
    height: 80rpx;
    border-top: 2rpx solid #efefef;
    font-size: 24rpx;
}

.info-table .table-list.remarks {
    height: auto;
    min-height: 80rpx;
    max-height: 160rpx;
}

.info-table .table-list:first-child {
    border-top: none;
}
.table-list .box-flex:first-child {
    border-right: 2rpx solid #efefef;
}
.table-list text{
    padding: 0 10rpx;
    font-size: 24rpx;
    color: #A0A0A0;
    flex-shrink: 0;
}
.info-table .textview-overflow {
    padding: 0 4rpx;
    line-height: 45rpx;
}

.footer-bar .service-btn{
    background: #fff;
    font-size: 26rpx;
    padding: 0;
    margin: 0;
    line-height: inherit;
    color: #56555f;
    width: 100rpx;
}

.footer-bar .service-btn::after{
    border: none;
}

.footer-bar .service-btn image{
    width: 30rpx;
}

.icon-uncollect {
    background-image: url('data:image/png;base64,');
}

.icon-collect {
    background-image: url('data:image/png;base64,');
}

.prodetail-navbar .action {
    color: #fff;
    background: #b39452;
}

.auction-state-desc .font-bold {
    margin: 0 6rpx;
}
.gray{
    background: #efefef;
}

.booked-shadow{
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background: rgba(0, 0, 0, .5);
  z-index: 100000;
}

.booked-container{
    width: 450rpx;
    padding: 40rpx 50rpx; 
    position: relative;
}

.booked-title{
    font-size: 32rpx;
    padding-bottom: 30rpx;
    border-bottom: 2rpx solid #ddd; 
    margin-bottom: 40rpx;
}

.booked-content{
    line-height: 60rpx;
    height: 60rpx;
    margin-bottom: 40rpx;
}

.booked-content input{
    height: 60rpx;
    line-height: 60rpx;
    border: 2rpx solid #ccc;
    flex-grow: 1;
    box-sizing: border-box;
    padding: 0 20rpx;
}

.booked-content .count-box .font-bold{
    font-size: 40rpx;
    border: 2rpx solid #ccc;
    height: 60rpx;
    width: 60rpx;
    line-height: 56rpx;
    box-sizing: border-box;
}

.booked-content .count-box .font-bold:first-child{
    margin-right: 50rpx;
}

.booked-content .count-box .font-bold:last-child{
    margin-left: 50rpx;
}
.booked-content .price-title,.booked-content .count-title{
    margin-right: 30rpx;
    flex-shrink: 0;
}

.booked-container .close-btn{
    position: absolute;
    top: 30rpx;
    right: 30rpx;
}

.booked-btn{
    margin-top: 20rpx;
    background: #942C15;
    padding: 15rpx 100rpx;
    color: #fff;
}
