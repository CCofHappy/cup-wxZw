.tools-bar{
  height: 64rpx;
  padding: 12rpx 20rpx;
  border-top: 2rpx solid #efefef;
  border-bottom: 2rpx solid #efefef;
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  z-index: 1000;
}
.container{
    margin-top: 110rpx;
}
.search-bar{
  width: 80%;
}

.search-bar input{
  width: 85%;
  background: #efefef;
  line-height: 64rpx;
  height: 64rpx;
  padding: 0 20rpx;
}

.search-bar .box-center{
  padding: 0 20rpx;
  border-right: 2rpx solid #efefef;
}

.sort-btn{
  width: 20%;
  line-height: 64rpx;
}

.acution-box{
  margin: 20rpx;
  position: relative;
}

.pro-content{
  margin: 0;
  padding: 10rpx 15rpx 15rpx 15rpx;
}

.pro-tips{
  height: 88rpx;
  line-height: 88rpx;
  border-bottom: 2rpx solid #efefef;
  padding: 0 20rpx 0 80rpx ;
}

.icon-tools{
  width: 24rpx;
  height: 24rpx; 
}

.go-history{
  position: fixed;
  bottom: 30rpx;
  right: 30rpx;
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
}

.icon-history{
    width:80rpx;
    height:80rpx;
}

.icon-search{
