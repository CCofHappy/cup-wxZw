/* address-list.wxss */
@import '../../component/nullTip/nullTip.wxss';
page{
  background: #efefef;
}

.address-box{
  margin: 20rpx;
  border-radius: 10rpx;
}

.address-name{
  font-size: 32rpx;
  padding: 20rpx 30rpx;
  border-bottom:2rpx solid #efefef;
}

.address-detail{
  font-size: 28rpx;
  padding: 30rpx;
  border-bottom:2rpx solid #efefef;
}

.address-tools{
  padding: 20rpx 30rpx;
  justify-content:  space-between;
}

.edit-btn{
  margin-right: 20rpx;
}

.add-btn{
  margin: 80rpx 30rpx;
}

.icon-edit{
  width: 28rpx;
  height: 28rpx;
  margin-right: 10rpx;
  background-image:url('data:image/png;base64,');
}

.icon-delete{
  width:28rpx;
  height: 28rpx;
  margin-right: 10rpx;
  background-image:url('data:image/png;base64,');
}