/* order-submit.wxss */
page{
  background: #efefef;
}

.position-box{
  height: 140rpx;
  margin-top: 10rpx;
}

.text-box{
  padding: 15rpx 0;
  width: 100%;
}

.text-box>view{
  height: 55rpx;
  line-height: 55rpx;
}

.pro-box{
  margin-top: 10rpx;
  padding: 20rpx;
  height: 160rpx;
}

.pro-box .img-box{
  height: 160rpx;
  width: 160rpx;
  margin-right: 20rpx;
}

.pro-box .img-box image{
  width: 100%;
  height: 100%;
}

.pro-box .bull{
  line-height: 160rpx;
}

.price-box{
  margin-top: 10rpx;
}

.price-box .text-right{
  padding: 0 30rpx;
  height: 88rpx;
  line-height: 88rpx;
  border-bottom: 2rpx solid #efefef;
}

.price-box .text-right:last-child{
  border: none;
}

.invoice-box{
  padding: 0 30rpx;
  margin-top: 10rpx;
  height: 88rpx;
  line-height: 88rpx;
}

.invoice-box {
  justify-content: space-between;
  border-bottom: 2rpx solid #efefef;
}

.invoice-content .items-list{
  border-bottom: 2rpx solid #efefef;
  justify-content: space-between;
}

.invoice-tips{
  padding: 20rpx 30rpx;
}

.bottom-bar {
  width: 100%;
  position: fixed;
  bottom: 0;
  height: 88rpx;
  line-height: 88rpx;
  z-index: 1000000;
}

.pay-btn{
  background: #C72202;
  color: #fff;
  padding: 0;
  border-radius: 0;
  font-size: 28rpx;
  line-height: 88rpx;
}

.icon-position{
  margin: 40rpx 20rpx 40rpx 0;
  width: 50rpx;
  height: 50rpx;
  background-image: url(data:image/png;base64,)
}

.check-icon-box{
  position: relative;
  height: 44rpx;
  width: 44rpx;
  border: 2rpx solid #c7c7cc;
  margin: 20rpx;
  border-radius: 50%;
}

.check-icon-box.choose{
  background: #C72202;
  border: 2rpx solid #C72202;
}

.icon-choose{
  height: 44rpx;
  width: 44rpx;
  background-size: 60%;
  position: absolute;
  top: 0;
  left: 0;
  background-image:url('data:image/png;base64,');
}