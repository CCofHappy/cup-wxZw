/* pay-deposit.wxss */
page{
  background: #efefef;
}

.pay-choose, .pay-type, .pay-rule, .pay-tips{
  margin-top:10rpx;
}

.pay-box{
  padding: 16rpx 30rpx;
  border-bottom: 2rpx solid #efefef;
  align-content: center;
}

.pay-box radio{
  opacity: 0;
}

.pay-choeck{
  line-height: 48rpx;
  padding-left: 70rpx;
  background-size: 50rpx;
  background-repeat: no-repeat;
  background-position: 0 center;
}

.tips .font-small{
  line-height: 44rpx;
}

.bond-btn{
  margin: 60rpx 30rpx;
  background: #C72202;
  color: #fff;
}

.pay-password-container{
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background: rgba(0, 0, 0, .5);
}

.pay-password-box{
  padding: 0 30rpx 0 30rpx;
  width: 480rpx;
  border-radius: 14rpx;
  position: relative;
}

.pay-title{
  font-size: 36rpx;
  padding: 15rpx 0;
  border-bottom: 2rpx solid #ddd;
}
.close-password-box{
    width:45rpx;
    height:45rpx;
    position:absolute;
    right:20rpx;
    top:26rpx;
}
.pay-value{
  font-size: 60rpx;
  margin-bottom: 20rpx;
}

.pay-value .font-small{
  font-size: 32rpx;
}

.password-box{
  width: 100%;
  overflow: hidden;
}

.password-box .fn-float{
    width: 16.66%;
    float: left;
    box-sizing: border-box;
    border: 2rpx solid #999;
    border-right: none;
    height: 80rpx;
}
.password-box .fn-float:last-child{
  border-right: 2rpx solid #999;
}
.pay-password-box .cancel{
  height: 88rpx;
  line-height: 88rpx;
  margin-top: 30rpx;
  border-top: 2rpx solid #ddd;
}

.password-box .bull{
  background: #56555F;
  height: 10rpx;
  width: 10rpx;
  border-radius: 50%;
}

