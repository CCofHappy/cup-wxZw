/* order-all.wxss */
@import '../../component/nullTip/nullTip.wxss';
page{
  background: #efefef;
}

.order-box{
  margin-top: 10rpx;
}

.order-num{
  padding: 0 30rpx;
  height: 88rpx;
  line-height: 88rpx;
  border-bottom: 2rpx solid #efefef;
}
.weui-tab__content .container{
    margin-bottom: 98rpx;
}

.auction-detail{
    height: 260rpx;
    box-sizing: border-box;
    padding: 20rpx 30rpx;
    border-bottom: 2rpx solid #eee;
}

.auction-img{
  width: 30%;
}

.auction-img image{
    width: 100%;
}

.auction-text{
  width: 60%;
  margin-left: 20rpx;
}

.auction-name{
  font-size: 32rpx;
  margin: 10rpx 0;
}

.btn-box{
  overflow: hidden;
  padding: 20rpx 30rpx;
}

.order-btn{
    display: block;
    line-height: 60rpx;
    width: 200rpx;
    height: 60rpx;
    border: 2rpx solid #C72202;
    margin-left: 30rpx;
    border-radius: 10rpx;
}

.order-btn.red{
    color: #fff;
    background: #C72202;
}

.bottom-bar {
  width: 100%;
  position: fixed;
  bottom: 0;
  height: 88rpx;
  line-height: 88rpx;
}

.check-icon-box{
  position: relative;
  height: 44rpx;
  width: 44rpx;
  border: 2rpx solid #c7c7cc;
  margin: 20rpx;
  border-radius: 50%;
  flex-shrink: 0;
}

.check-icon-box.choose{
  background: #C72202;
  border: 2rpx solid #C72202;
}

.pay-btn{
  background: #C72202;
  color: #fff;
}

.goto-cancel-order{
  position: fixed;
  bottom: 120rpx;
  right: 30rpx;
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
}

.font-gray-more .font-red{
    font-size: 30rpx;
    color: #942C15;
}

.icon-cancel-order{
    width:80rpx;
    height:80rpx;
}

.icon-choose{
  height: 44rpx;
  width: 44rpx;
  background-size: 60%;
  position: absolute;
  top: 0;
  left: 0;
  background-image:url('data:image/png;base64,');
}