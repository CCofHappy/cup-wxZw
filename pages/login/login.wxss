.logo{
  width: 280rpx;
  margin: 140rpx auto 60rpx auto;
}
.code-btn{
  font-size: 24rpx;
  flex-shrink: 0;
}
.login-box{
  padding:0 80rpx;
}

.login-list{
  padding: 0 0 0 80rpx;
  background-position:5% center ;
  background-size: 32rpx;
  background-repeat: no-repeat;
  border-bottom: 2rpx solid #e6e6e6;
  margin-bottom:20rpx; 
  line-height: 70rpx;
  height: 70rpx;
}

.login-list input{
  line-height: 70rpx;
  height: 70rpx;
  font-size: 30rpx;
}

.login-box button{
  margin-top: 80rpx;
  background: #C72202;
  border-radius: 6rpx;
}
.login-box .button-hover {
  background-color: #C72202;
}

.orther{
  margin-top: 20rpx; 
  justify-content: space-between;
}

.login-list.id{
  background-image:url('data:image/png;base64,');
}

.login-list.tel{
  background-image:url('data:image/png;base64,');
}

.login-list.pwd{
  background-image:url('data:image/png;base64,');
}

.login-list.code{
  justify-content: space-between;
  background-image:url('data:image/png;base64,');
}