.null-tip {
    height: 400rpx;
    margin: 80rpx 0 0;
    background-image: url(data:image/png;base64,);
    background-repeat: no-repeat;
    background-position: center;
    background-size: 17%;
    line-height: 600rpx;
    text-align: center;
    color: #ccc;
    font-size: 24rpx;
}

.null-action {
    margin: 0 auto;
    width: 200rpx;
    height: 60rpx;
    line-height: 60rpx;
    text-align: center;
    border-radius: 8rpx;
    border: 1px solid #c89664;
    color: #c89664;
}

.null-action:active {
    background-color: #c89664;
    color: #fff;
}

.refresh-btn{
    border: 2rpx solid #ddd;
    border-radius: 10rpx;
    width: 200rpx;
    height: 70rpx;
    line-height: 70rpx;
    text-align: center;
    margin: auto;
}